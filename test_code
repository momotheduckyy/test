from sample_code import add_numbers

# Test Case 1: Add two positive numbers
result1 = add_numbers(2, 3)
print(f"Test 1: 2 + 3 = {result1}, Expected: 5, Pass: {result1 == 5}")

# Test Case 2: Add a positive and a negative number
result2 = add_numbers(10, -5)
print(f"Test 2: 10 + (-5) = {result2}, Expected: 5, Pass: {result2 == 5}")

# Test Case 3: Add two zeros
result3 = add_numbers(0, 0)
print(f"Test 3: 0 + 0 = {result3}, Expected: 0, Pass: {result3 == 0}")
